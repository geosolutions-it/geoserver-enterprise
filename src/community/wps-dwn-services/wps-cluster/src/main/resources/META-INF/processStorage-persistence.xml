<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" 
    xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd ">
        
  <persistence-unit name="processStorage" transaction-type="RESOURCE_LOCAL">
    <properties>
      <property name="hibernate.show_sql" value="false"/>
    </properties>
    <!-- when adding/removing classes, please also keep aligned the src/test/resource/hibernate.cfg.xml file -->
    <class>org.geoserver.wps.executor.storage.model.ProcessDescriptor</class>
  </persistence-unit>
</persistence>
