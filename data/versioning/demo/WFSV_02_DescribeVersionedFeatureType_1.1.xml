<!--
    This call describes the feature type as "versioned", that is, extending a different
    base type, AbstractVersionedFetureType, that compared to the usual GML feature
    has 4 extra attributes informing you about the last change occurred on the feature:
    version, author, date and transaction message   
 -->
<DescribeVersionedFeatureType
  version="1.1.0"
  service="WFSV" versioned="true"
  xmlns="http://www.opengis.net/wfsv"
  xmlns:wfs="http://www.opengis.net/wfs"
  xmlns:topp="http://www.openplans.org/topp"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/wfsv http://localhost:8080/geoserver/schemas/wfs/1.1.0/wfsv.xsd">
    <wfs:TypeName>topp:archsites</wfs:TypeName>
</DescribeVersionedFeatureType>